{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-gateway",
  "projects": {
    "analytics-service": {
      "type": "application",
      "root": "apps/analytics-service",
      "entryFile": "main",
      "sourceRoot": "apps/analytics-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analytics-service/tsconfig.app.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth-guard": {
      "type": "library",
      "root": "libs/auth-guard",
      "entryFile": "index",
      "sourceRoot": "libs/auth-guard/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth-guard/tsconfig.lib.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "billing-service": {
      "type": "application",
      "root": "apps/billing-service",
      "entryFile": "main",
      "sourceRoot": "apps/billing-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/billing-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "customization-service": {
      "type": "application",
      "root": "apps/customization-service",
      "entryFile": "main",
      "sourceRoot": "apps/customization-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/customization-service/tsconfig.app.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "integration-service": {
      "type": "application",
      "root": "apps/integration-service",
      "entryFile": "main",
      "sourceRoot": "apps/integration-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/integration-service/tsconfig.app.json"
      }
    },
    "multi-tenant-saas": {
      "type": "application",
      "root": "apps/multi-tenant-saas",
      "entryFile": "main",
      "sourceRoot": "apps/multi-tenant-saas/src",
      "compilerOptions": {
        "tsConfigPath": "apps/multi-tenant-saas/tsconfig.app.json"
      }
    },
    "rabbitmq": {
      "type": "library",
      "root": "libs/rabbitmq",
      "entryFile": "index",
      "sourceRoot": "libs/rabbitmq/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rabbitmq/tsconfig.lib.json"
      }
    },
    "tenant-service": {
      "type": "application",
      "root": "apps/tenant-service",
      "entryFile": "main",
      "sourceRoot": "apps/tenant-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/tenant-service/tsconfig.app.json"
      }
    }
  }
}